<%-  include('../admin_layout/header.ejs') %>

    <div class="container">
        <h4>Total users : <%= userList.length %></h4>
        <table class="table">
            <thead>
                <tr>
                    <th>Sr No.</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Contact</th>
                    <th>Created at</th>
                    <th>Contact List</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                <% userList.forEach(function(users,index){ %>
                    <tr>
                        <td><b>#<%= index + 1 %></b></td>
                        <td><%= users.name %></td>
                        <td><%= users.email %> </td>
                        <td><%= users.contact %> </td>
                        <td><%= moment(users.created_at).format("MM/DD/YYYY HH:mm")  %> </td>
                        <td>
                            <a href="" class="btn btn-warning text-dark"><i class="fa-solid fa-eye"></i></a>
                        </td>
                        <td>
                            <a href="" class="btn btn-info text text-light"><i class="fa-solid fa-pen-to-square"></i></a>
                            <a href="" class="btn btn-danger"><i class="fa-solid fa-trash"></i></a>
                        </td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
<%- include('../admin_layout/footer.ejs') %>
